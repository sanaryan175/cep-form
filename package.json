{
  "name": "cep-survey-app",
  "version": "1.0.0",
  "description": "MERN app for collecting community feedback on financial awareness",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
    "nodemailer": "^8.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "mern",
    "survey",
    "financial-awareness",
    "mongodb"
  ],
  "author": "",
  "license": "MIT"
}
